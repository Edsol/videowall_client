generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database.db"
}

model display {
  id       Int     @id @default(autoincrement())
  port     String  @unique
  primary  Boolean @default(false)
  width    Int?
  height   Int?
  left     Int
  top      Int
  position String
}
